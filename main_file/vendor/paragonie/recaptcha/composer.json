{
    "name": "paragonie/recaptcha",
    "description": "A fork of Google's PHP ReCAPTCHA Client with Proxy support.",
    "type": "library",
    "keywords": ["recaptcha", "captcha", "spam", "abuse", "proxy"],
    "homepage": "http://www.google.com/recaptcha/",
    "license": "BSD-3-Clause",
    "support": {
        "forum": "https://groups.google.com/forum/#!forum/recaptcha",
        "source": "https://github.com/google/recaptcha"
    },
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "vimeo/psalm": "^0|^1"
    },
    "autoload": {
        "psr-4": {
            "ReCaptcha\\": "src/ReCaptcha"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
